{
  "name": "@aibos/bioskin",
  "version": "2.1.0",
  "description": "BioSkin 2.1 - Directive-Based UI Architecture (CONT_10 v2.1)",
  "type": "module",
  "sideEffects": false,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "import": "./src/server.ts"
    },
    "./table": {
      "types": "./src/table.ts",
      "import": "./src/table.ts"
    },
    "./chart": {
      "types": "./src/chart.ts",
      "import": "./src/chart.ts"
    },
    "./kanban": {
      "types": "./src/kanban.ts",
      "import": "./src/kanban.ts"
    },
    "./calendar": {
      "types": "./src/calendar.ts",
      "import": "./src/calendar.ts"
    },
    "./gantt": {
      "types": "./src/gantt.ts",
      "import": "./src/gantt.ts"
    },
    "./form": {
      "types": "./src/form.ts",
      "import": "./src/form.ts"
    },
    "./layout": {
      "types": "./src/layout.ts",
      "import": "./src/layout.ts"
    },
    "./atoms": {
      "types": "./src/atoms.ts",
      "import": "./src/atoms.ts"
    }
  },
  "typesVersions": {
    "*": {
      "server": [
        "./src/server.ts"
      ],
      "table": [
        "./src/table.ts"
      ],
      "chart": [
        "./src/chart.ts"
      ],
      "kanban": [
        "./src/kanban.ts"
      ],
      "calendar": [
        "./src/calendar.ts"
      ],
      "gantt": [
        "./src/gantt.ts"
      ],
      "form": [
        "./src/form.ts"
      ],
      "layout": [
        "./src/layout.ts"
      ],
      "atoms": [
        "./src/atoms.ts"
      ]
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:headed": "vitest --browser.headless=false",
    "analyze": "node scripts/analyze-bundle.mjs",
    "analyze:ci": "node scripts/analyze-bundle.mjs --ci",
    "size-check": "node scripts/analyze-bundle.mjs --ci"
  },
  "peerDependencies": {
    "lucide-react": ">=0.400.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "zod": "^3.0.0 || ^4.0.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.13",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "jotai": "^2.16.0",
    "motion": "^12.23.26",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.68.0",
    "react-use-measure": "^2.1.7",
    "server-only": "^0.0.1",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "esbuild": "^0.24.2",
    "@radix-ui/react-slot": "^1.2.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "axe-core": "^4.11.0",
    "class-variance-authority": "^0.7.1",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.474.0",
    "next-themes": "^0.4.6",
    "playwright": "^1.57.0",
    "react-day-picker": "^8.10.1",
    "react-resizable-panels": "^2.1.9",
    "recharts": "^2.15.4",
    "sonner": "^1.7.4",
    "typescript": "~5.6.2",
    "vaul": "^1.1.2",
    "vitest": "^4.0.15",
    "vitest-axe": "^0.1.0",
    "zod": "^4.1.13"
  }
}