{
  "name": "@aibos/db",
  "version": "1.0.0",
  "description": "AI-BOS Data Fabric - Database Governance & Orchestration Layer",
  "private": true,
  "type": "module",
  "scripts": {
    "migrate": "tsx scripts/migrate.ts",
    "migrate:kernel": "tsx scripts/migrate.ts --schema kernel",
    "migrate:finance": "tsx scripts/migrate.ts --schema finance",
    "migrate:config": "tsx scripts/migrate.ts --schema config",
    "seed:kernel": "tsx seeds/kernel/seed-happy-path.ts",
    "seed:finance": "tsx seeds/finance/seed-demo-corp.ts",
    "seed:all": "pnpm seed:kernel && pnpm seed:finance",
    "validate": "tsx tools/validate-schema.ts",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:reset": "docker-compose down -v && docker-compose up -d"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "bcryptjs": "^3.0.3",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2"
  }
}